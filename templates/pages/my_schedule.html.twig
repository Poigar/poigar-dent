{% include 'header.html.twig' %}
{% include 'navbars/navbar.html.twig' %}

<div class="wrapper__main margin__under-navbar">
    <div class="container">
        <div class="card mb-5">
            <div class="card-body">
                <table class="table table-striped table-bordered">
                    <tbody>

                        {% for key in range(0,23) %}
                            
                            <tr class="employee">
                                <td width="10%" align="center" scope="row">{% if(key<10) %}0{% endif %}{{ key }}:00</td>
                                <td scope="row"> Random thing </td>
                            </tr>
                            
                        {% endfor %}

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
